%table.table.table-striped
	%tr
		%th Bloque
		%th Lunes
		%th Martes
		%th Miércoles
		%th Jueves
		%th Viernes
	-for b in @blocks
		%tr
			%td
				=b.name
			-for s in @shifts
				- if s.block === b	
					%td
						-unless s.meeting	
							- for c in s.checkins
								= c.employee.name
								%br
						-else
							Reunión 